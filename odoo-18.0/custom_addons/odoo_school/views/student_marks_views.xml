<odoo>
    <record id="view_student_marks_list" model="ir.ui.view">
        <field name="name">school.student.marks.list</field>
        <field name="model">school.student.marks</field>
        <field name="arch" type="xml">
            <list>
                <field name="marks_entry_id"/>
                <field name="student_id"/>
                <field name="subject_id"/>
                <field name="marks"/>
                <field name="previous_marks"/>
            </list>
        </field>
    </record>
    <record id="action_student_marks" model="ir.actions.act_window">
        <field name="name">Student Marks</field>
        <field name="res_model">school.student.marks</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>
